package app_Java_8;

public class A {
	int x = 10;
	public static void main(String[] args) {
		A a1 = new A();
		a1.x = 20;
		System.out.println(a1.x);
		
		A a2 = new A();
		System.out.println(a2.x);
		
		A a3 = new A();
		System.out.println(a3.x);
		
		A a4 = new A();
		a4.x = 30;
		System.out.println(a4.x);
	}
}